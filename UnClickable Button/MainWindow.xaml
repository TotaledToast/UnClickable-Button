<Window x:Class="UnClickable_Button.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnClickable_Button"
        mc:Ignorable="d"
        Title="Click Me!" Height="450" Width="800">
    <Grid x:Name="Window">
        <Button x:Name="UnClickable" Content="Click Me!" HorizontalAlignment="Left" Height="100" Margin="291,222,0,0" VerticalAlignment="Top" Width="200" Click="UnClickable_Click" ClickMode="Hover"/>

    </Grid>
</Window>
